{
  "name": "@projectx/listing-service",
  "version": "1.0.0",
  "private": true,
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "start": "node -r dotenv/config dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --coverageThreshold={\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "docs": "typedoc --out docs src"
  },
  "dependencies": {
    "@projectx/common": "1.0.x",
    "@projectx/database": "1.0.x",
    "@elastic/elasticsearch": "8.9.0",
    "express": "4.18.2",
    "knex": "2.5.1",
    "pg": "8.11.2",
    "pino": "8.16.1",
    "@opentelemetry/api": "1.7.0",
    "cors": "2.8.x",
    "helmet": "7.1.0",
    "compression": "1.7.x",
    "express-rate-limit": "6.9.x",
    "express-validator": "7.0.1",
    "express-timeout-handler": "2.2.2",
    "express-sanitizer": "1.0.6",
    "express-error-handler": "1.1.x",
    "morgan": "1.10.x",
    "winston": "^3.11.0",
    "dotenv": "^16.0.0",
    "ioredis": "5.3.2",
    "prom-client": "14.2.0",
    "redis-lock": "^1.0.0",
    "node-cache-manager": "^3.0.0",
    "dd-trace": "3.32.0"
  },
  "devDependencies": {
    "typescript": "5.0.4",
    "@types/express": "4.17.17",
    "@types/cors": "2.8.13",
    "@types/morgan": "1.9.4",
    "@types/jest": "29.5.3",
    "@types/pg": "8.10.2",
    "@types/node": "18.15.0",
    "jest": "29.6.2",
    "ts-jest": "29.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "8.54.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "prettier": "3.1.0",
    "nodemon": "3.0.1",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "testcontainers": "9.9.1",
    "@testing-library/database-mock": "1.0.0",
    "jest-mock-extended": "3.0.4",
    "nock": "13.3.3"
  },
  "peerDependencies": {
    "@projectx/common": "1.0.x",
    "@projectx/database": "1.0.x"
  }
}