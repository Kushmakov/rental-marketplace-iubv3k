{
  "name": "Project X Rental Marketplace",
  "short_name": "Project X",
  "description": "Comprehensive rental marketplace platform for property listings, applications, and payments",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFFFFF",
  "theme_color": "#000000",
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "16x16 32x32 48x48",
      "type": "image/x-icon",
      "purpose": "any"
    },
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "categories": [
    "real estate",
    "rental",
    "property management",
    "finance",
    "productivity"
  ],
  "scope": "/",
  "prefer_related_applications": false,
  "shortcuts": [
    {
      "name": "Search Properties",
      "url": "/search",
      "description": "Search available rental properties"
    },
    {
      "name": "My Applications", 
      "url": "/applications",
      "description": "View your rental applications"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+property",
      "url": "/property?id=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  }
}